  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run build tool",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/main.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run publisher script",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/publisher/main.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Run report viewer",
      "skipFiles": ["<node_internals>/**"],
      "program": "${workspaceFolder}/dist/report-viewer/main.js"
    }
  ]
}
